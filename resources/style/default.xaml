	<Style
		TargetType="{x:Type TextBlock}"
		x:Key="{x:Type TextBlock}">
		<Setter
			Property="FontFamily"
			Value="{StaticResource BrownPro}" />
	</Style>
	<Style
		TargetType="{x:Type Run}"
		x:Key="{x:Type Run}">
		<Setter
			Property="TextDecorations"
			Value="{x:Null}" />
		<Style.Triggers>
			<Trigger
				Property="ContentElement.IsMouseOver"
				Value="True">
				<Setter
					Property="TextDecorations"
					Value="Underline" />
			</Trigger>
		</Style.Triggers>
	</Style>
	<Style
		TargetType="{x:Type Button}"
		x:Key="ButtonAzuro">
		<Setter
			Property="Background"
			Value="{StaticResource BrushAzuro}" />
		<Setter
			Property="Foreground"
			Value="#FFFFFFFF" />
		<Setter
			Property="Width"
			Value="192" />
		<Setter
			Property="Height"
			Value="32" />
		<Setter
			Property="BorderThickness"
			Value="0" />
		<Setter
			Property="FontSize"
			Value="12" />
		<Setter
			Property="FontWeight"
			Value="Regular" />
		<Setter
			Property="UIElement.SnapsToDevicePixels"
			Value="True" />
		<Setter
			Property="FrameworkElement.Cursor"
			Value="Arrow" />
		<Setter
			Property="Template">
			<Setter.Value>
				<ControlTemplate
					TargetType="{x:Type Button}">
					<Border
						CornerRadius="0"
						Background="{TemplateBinding Background}">
						<Grid>
							<ContentPresenter
								Content="{TemplateBinding Content}"
								HorizontalAlignment="Center"
								VerticalAlignment="Center"
								Margin="0,3,0,0" />
						</Grid>
					</Border>
					<ControlTemplate.Triggers>
						<Trigger
							Property="UIElement.IsMouseOver"
							Value="True">
							<Setter
								Property="Background"
								Value="{StaticResource BrushAzuroHover}" />
							<Setter
								Property="Foreground"
								Value="#FFFFFFFF" />
						</Trigger>
						<Trigger
							Property="IsPressed"
							Value="True">
							<Setter
								Property="Background"
								Value="{StaticResource BrushAzuro}" />
							<Setter
								Property="Foreground"
								Value="#FFFFFFFF" />
						</Trigger>
						<Trigger
							Property="IsEnabled"
							Value="False">
							<Setter
								Property="Background"
								Value="{StaticResource BrushDisabledBackground}" />
							<Setter
								Property="Foreground"
								Value="{StaticResource BrushDisabledForeground}" />
						</Trigger>
					</ControlTemplate.Triggers>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>
	<Style
		TargetType="{x:Type Button}"
		x:Key="ButtonText">
		<Setter
			Property="Background"
			Value="#00FFFFFF" />
		<Setter
			Property="Foreground"
			Value="{StaticResource BrushAzuro}" />
		<Setter
			Property="BorderThickness"
			Value="0" />
		<Setter
			Property="FontWeight"
			Value="Light" />
		<Setter
			Property="UIElement.SnapsToDevicePixels"
			Value="True" />
		<Setter
			Property="FrameworkElement.Cursor"
			Value="Hand" />
		<Setter
			Property="Template">
			<Setter.Value>
				<ControlTemplate
					TargetType="{x:Type Button}">
					<ControlTemplate.Triggers>
						<Trigger
							Property="UIElement.IsMouseOver"
							Value="True">
							<Setter
								Property="Background"
								Value="#00FFFFFF" />
							<Setter
								Property="Foreground"
								Value="{StaticResource BrushAzuroHover}" />
						</Trigger>
					</ControlTemplate.Triggers>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
		<Setter
			Property="Template">
			<Setter.Value>
				<ControlTemplate
					TargetType="{x:Type Button}">
					<TextBlock
						Text="{TemplateBinding Content}"
						FontSize="14"
						TextWrapping="Wrap" />
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>
	<Style
		TargetType="{x:Type Button}"
		x:Key="ButtonCoal">
		<Setter
			Property="Background"
			Value="{StaticResource BrushCoal}" />
		<Setter
			Property="Foreground"
			Value="#FFFFFFFF" />
		<Setter
			Property="Width"
			Value="192" />
		<Setter
			Property="Height"
			Value="32" />
		<Setter
			Property="BorderThickness"
			Value="0" />
		<Setter
			Property="FontSize"
			Value="12" />
		<Setter
			Property="FontWeight"
			Value="Regular" />
		<Setter
			Property="UIElement.SnapsToDevicePixels"
			Value="True" />
		<Setter
			Property="FrameworkElement.Cursor"
			Value="Arrow" />
		<Setter
			Property="Template">
			<Setter.Value>
				<ControlTemplate
					TargetType="{x:Type Button}">
					<Border
						CornerRadius="0"
						Background="{TemplateBinding Background}">
						<Grid>
							<ContentPresenter
								Content="{TemplateBinding Content}"
								HorizontalAlignment="Center"
								VerticalAlignment="Center"
								Margin="0,3,0,0" />
						</Grid>
					</Border>
					<ControlTemplate.Triggers>
						<Trigger
							Property="UIElement.IsMouseOver"
							Value="True">
							<Setter
								Property="Background"
								Value="{StaticResource BrushCoalHover}" />
							<Setter
								Property="Foreground"
								Value="#FFFFFFFF" />
						</Trigger>
						<Trigger
							Property="IsPressed"
							Value="True">
							<Setter
								Property="Background"
								Value="{StaticResource BrushCoal}" />
							<Setter
								Property="Foreground"
								Value="#FFFFFFFF" />
						</Trigger>
					</ControlTemplate.Triggers>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>
	<LinearGradientBrush
		StartPoint="0,0"
		EndPoint="1,0"
		x:Key="ProgressBarIndicatorAnimatedFill">
		<GradientBrush.GradientStops>
			<GradientStopCollection>
				<GradientStop
					Color="#00a9e0"
					Offset="0" />
				<GradientStop
					Color="#00aee0"
					Offset="0.2" />
				<GradientStop
					Color="#00ebbf"
					Offset="0.4" />
				<GradientStop
					Color="#00ebbf"
					Offset="0.6" />
				<GradientStop
					Color="#00aee0"
					Offset="0.8" />
				<GradientStop
					Color="#00a9e0"
					Offset="1" />
			</GradientStopCollection>
		</GradientBrush.GradientStops>
	</LinearGradientBrush>
	<Style
		TargetType="{x:Type ProgressBar}"
		x:Key="{x:Type ProgressBar}">
		<Setter
			Property="Width"
			Value="328" />
		<Setter
			Property="HorizontalAlignment"
			Value="Center" />
		<Setter
			Property="Background"
			Value="#00000000" />
		<Setter
			Property="BorderBrush"
			Value="#fff" />
		<Setter
			Property="Template">
			<Setter.Value>
				<ControlTemplate
					TargetType="{x:Type ProgressBar}">
					<Grid>
						<Rectangle
							Name="PART_Track"
							Width="328"
							Height="4"
							Fill="{StaticResource BrushProgressTrackBackground}" />
						<Grid
							Name="PART_Indicator"
							Width="328"
							Height="4"
							HorizontalAlignment="Left"
							Background="{StaticResource BrushAzuro}">
							<Grid
								ClipToBounds="True">
								<Rectangle
									Name="PART_GlowRect"
									Width="137"
									Height="4"
									HorizontalAlignment="Left"
									Margin="-137,0,137,0"
									Fill="{StaticResource ProgressBarIndicatorAnimatedFill}" />
							</Grid>
						</Grid>
					</Grid>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
		<Style.Triggers>
			<Trigger
				Property="IsEnabled"
				Value="False">
				<TriggerBase.EnterActions>
					<BeginStoryboard>
						<Storyboard>
							<DoubleAnimation
								Storyboard.TargetProperty="Width"
								RepeatBehavior="1x"
								From="328"
								To="0"
								Duration="0:0:0.25" />
						</Storyboard>
					</BeginStoryboard>
				</TriggerBase.EnterActions>
			</Trigger>
		</Style.Triggers>
	</Style>
	<Style
		TargetType="{x:Type Grid}"
		x:Key="GridContainerInfo">
		<Style.Triggers>
			<Trigger
				Property="Visibility"
				Value="Hidden">
				<TriggerBase.EnterActions>
					<BeginStoryboard>
						<Storyboard>
							<DoubleAnimation
								Storyboard.TargetProperty="Opacity"
								From="1.0"
								To="0.0"
								Duration="0:0:0.25" />
						</Storyboard>
					</BeginStoryboard>
				</TriggerBase.EnterActions>
			</Trigger>
			<Trigger
				Property="Visibility"
				Value="Visible">
				<TriggerBase.EnterActions>
					<BeginStoryboard>
						<Storyboard>
							<DoubleAnimation
								Storyboard.TargetProperty="Opacity"
								From="0.0"
								To="1.0"
								Duration="0:0:0.25" />
						</Storyboard>
					</BeginStoryboard>
				</TriggerBase.EnterActions>
			</Trigger>
		</Style.Triggers>
	</Style>
	<Style
		TargetType="{x:Type Grid}"
		x:Key="ModalBackground">
		<Style.Triggers>
			<Trigger
				Property="Visibility"
				Value="Visible">
				<TriggerBase.EnterActions>
					<BeginStoryboard>
						<Storyboard>
							<DoubleAnimation
								Storyboard.TargetProperty="Opacity"
								From="0.0"
								To="1.0"
								Duration="0:0:0.1" />
						</Storyboard>
					</BeginStoryboard>
				</TriggerBase.EnterActions>
			</Trigger>
		</Style.Triggers>
	</Style>
	<Style
		TargetType="{x:Type Image}"
		x:Key="Spinner">
		<Style.Triggers>
			<Trigger
				Property="Visibility"
				Value="Hidden">
				<Setter
					Property="Width"
					Value="0" />
				<Setter
					Property="Height"
					Value="0" />
			</Trigger>
			<Trigger
				Property="Visibility"
				Value="Visible">
				<TriggerBase.EnterActions>
					<BeginStoryboard>
						<Storyboard
							BeginTime="0:0:1"
							RepeatBehavior="1x">
							<DoubleAnimation
								Storyboard.TargetProperty="Opacity"
								From="0.0"
								To="1.0"
								Duration="0:0:0.5" />
							<DoubleAnimation
								Storyboard.TargetProperty="Width"
								From="0"
								To="32"
								Duration="0:0:1" />
							<DoubleAnimation
								Storyboard.TargetProperty="Height"
								From="0"
								To="32"
								Duration="0:0:1" />
						</Storyboard>
					</BeginStoryboard>
				</TriggerBase.EnterActions>
			</Trigger>
		</Style.Triggers>
	</Style>
</ResourceDictionary>
