<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<FontFamily
		x:Key="BrownPro">pack://application:,,,/Resources/fonts/#BrownPro</FontFamily>
	<SolidColorBrush
		Color="#00a9e0"
		x:Key="BrushAzuro" />
	<SolidColorBrush
		Color="#1ab1e3"
		x:Key="BrushAzuroHover" />
	<SolidColorBrush
		Color="#393939"
		x:Key="BrushCoal" />
	<SolidColorBrush
		Color="#4d4d4d"
		x:Key="BrushCoalHover" />
	<SolidColorBrush
		Color="#ff2947"
		x:Key="BrushKirche" />
	<SolidColorBrush
		Color="#999999"
		x:Key="BrushTextGrey" />
	<SolidColorBrush
		Color="#ffb401"
		x:Key="BrushWarning" />
	<SolidColorBrush
		Color="#ff4464"
		x:Key="BrushError" />
	<SolidColorBrush
		Color="#1a1a1a"
		x:Key="BrushDisabledBackground" />
	<SolidColorBrush
		Color="#5d5d5d"
		x:Key="BrushDisabledForeground" />
	<SolidColorBrush
		Color="#003343"
		x:Key="BrushProgressTrackBackground" />
